#include<bits/stdc++.h>
using namespace std;
#define optimized() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n'
int main() {
    optimized();
    int t;
    cin>>t;
    while(t--){
       int n;
       cin>>n;
       int a;
       int sum =0;
       for(int i=0;i<n;i++){
        cin>>a;
        sum=(sum|a);
       }
       cout<<sum<<endl;
    }
    return 0;
}

