#include<bits/stdc++.h>
using namespace std;
#define optimized() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n'

int main() {
    optimized();
    string s;
    cin>>s;
    bool flag=false;
    for(int i=0;i<s.size();i++){
        if(s[i]=='1' && s[i+1]=='1' && s[i+2]=='1' && s[i+3]=='1' && s[i+4]=='1' && s[i+5]=='1' && s[i+6]=='1'){
            flag=true;
        }
        else if(s[i]=='0' && s[i+1]=='0' && s[i+2]=='0' && s[i+3]=='0' && s[i+4]=='0' && s[i+5]=='0' && s[i+6]=='0'){
            flag=true;
        }
    }

    if(flag==true) cout<<"YES"<<endl;
    else cout<<"NO"<<endl;
    
    return 0;
}

